{
  "env": {
    "doc": "The applicaton environment.",
    "format": [
      "production",
      "development",
      "test"
    ],
    "default": "development",
    "env": "NODE_ENV"
  },
  "logs": {
    "path": "../../logs/app.log"
  },
  "port": {
    "doc": "The port to bind.",
    "format": "port",
    "default": 8080,
    "env": "PORT"
  }
}